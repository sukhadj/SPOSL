package com.sukhad;


class Process implements Comparable<Process>
{
	String name;
	int arrival_time;
	int burst_time;
	int tw;
	int tat;

	Process()
	{
		this.name = "";
		this.arrival_time = 0;
	}
	
	@Override
	public int compareTo(Process process) {
	
		if(this.burst_time > process.burst_time)
		{
			return 1;
		}
		else if(this.burst_time < process.burst_time)
		{
			return -1;
		}
		return 0;
	}
	

}

public class SJF {
	
	
	public void main(String args[])
	{
		
	}
}
